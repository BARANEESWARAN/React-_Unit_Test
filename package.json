{
  "name": "jestproctice",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jestproctice": "file:",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage --watchAll=false --collectCoverageFrom=\"src/components/**/*.{js,jsx}\" --collectCoverageFrom=\"!src/components/**/*.{types,constants,stories,reportWebVitals,test,spec,index}\"",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .igitignore --write \"**/*.{js,jsx,css,scss}\"",
    "prepare": "husky && husky install"
  },
  "eslintConfig": {
    "plugins": [
      "jest-dom"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jest-dom/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "exact": "^1.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.3.3"
  }
}
